{
  "name": "vslang-br",
  "displayName": "vslang-br",
  "description": "Language support for Business Rules!",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.70.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "snippets": [
      {
        "language": "br",
        "path": "snippets/fileio.json"
      },
      {
        "language": "br",
        "path": "snippets/lexi.json"
      },
      {
        "language": "br",
        "path": "snippets/loops.json"
      }
    ],
    "languages": [{
      "id": "br",
      "aliases": ["Business Rules!", "br"],
      "extensions": [".brs",".wbs"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "br",
      "scopeName": "source.br",
      "path": "./syntaxes/br.tmLanguage.json"
    }]
  }
}
